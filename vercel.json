{
  "functions": {
    "api/*.go": {
      "runtime": "@vercel/go@1.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.go"
    }
  ],
  "buildCommand": "go build -o api/main api/main.go",
  "outputDirectory": ".",
  "installCommand": "go mod download"
} 